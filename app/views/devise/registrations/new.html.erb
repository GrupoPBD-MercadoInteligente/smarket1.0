<h2>Registro</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :usuario_nombre_usuario %><br />
  <%= f.email_field :usuario_nombre_usuario, autofocus: true %></div>

  <div><%= f.label :usuario_contrasena %><br />
    <%= f.password_field :usuario_contrasena, autocomplete: "off" %></div>

  <div><%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %></div>

  <div><%= f.submit "Registrarse" %></div>
<% end %>

<%= render "devise/shared/links" %>
